{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'MyMain',\n\n  data() {\n    return {\n      form: {\n        id: \"\",\n        name: \"\"\n      },\n      users: []\n    };\n  },\n\n  methods: {\n    getItem() {\n      axios.get(\"http://localhost:3000/users\").then(response => {\n        this.users = response.data;\n        console.log(\"data\", response.data);\n      }).catch(error => {\n        console.log(\"Error Get Data \", error);\n      });\n    },\n\n    postItem() {\n      axios.post(\"http://localhost:3000/users\", this.form).then(() => {\n        this.getItem();\n        this.form.id = \"\";\n        this.form.name = \"\";\n        alert(\"Data berhasil dipost\");\n      }).catch(error => {\n        console.log(\"Error Get Data \", error);\n      });\n    }\n\n  },\n\n  mounted() {\n    this.getItem();\n  }\n\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,KAAP,MAAkB,OAAlB;AACI,eAAe;EACXC,IAAI,EAAE,QADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,IAAI,EAAC;QACDC,EAAE,EAAC,EADF;QAEDH,IAAI,EAAC;MAFJ,CADF;MAKHI,KAAK,EAAC;IALH,CAAP;EAOH,CAVU;;EAYXC,OAAO,EAAC;IACJC,OAAO,GAAE;MACLP,KAAK,CAACQ,GAAN,CAAU,6BAAV,EACCC,IADD,CACOC,QAAD,IAAc;QAChB,KAAKL,KAAL,GAAaK,QAAQ,CAACR,IAAtB;QACAS,OAAO,CAACC,GAAR,CAAY,MAAZ,EAAoBF,QAAQ,CAACR,IAA7B;MACH,CAJD,EAKCW,KALD,CAKQC,KAAD,IAAW;QACdH,OAAO,CAACC,GAAR,CAAY,iBAAZ,EAA+BE,KAA/B;MACH,CAPD;IAQH,CAVG;;IAWJC,QAAQ,GAAE;MACNf,KAAI,CACHgB,IADD,CACM,6BADN,EACqC,KAAKb,IAD1C,EAECM,IAFD,CAEM,MAAM;QACR,KAAKF,OAAL;QACA,KAAKJ,IAAL,CAAUC,EAAV,GAAc,EAAd;QACA,KAAKD,IAAL,CAAUF,IAAV,GAAgB,EAAhB;QACAgB,KAAK,CAAC,sBAAD,CAAL;MACH,CAPD,EAQCJ,KARD,CAQQC,KAAD,IAAW;QACdH,OAAO,CAACC,GAAR,CAAY,iBAAZ,EAA+BE,KAA/B;MACH,CAVD;IAWH;;EAvBG,CAZG;;EAsCXI,OAAO,GAAG;IACN,KAAKX,OAAL;EACJ;;AAxCW,CAAf","names":["axios","name","data","form","id","users","methods","getItem","get","then","response","console","log","catch","error","postItem","post","alert","mounted"],"sourceRoot":"","sources":["D:\\Semester 3\\Proyek 3\\CobaVue\\CrudVue\\crud\\src\\Views\\MyMain.vue"],"sourcesContent":["<template>\r\n    <h1>Hallo</h1>\r\n    <div>\r\n        <h2>Add Users</h2>\r\n        <from @submit.prevent=\"postItem\">\r\n            <input type=\"hidden\" v-model=\"form.id\" placeholder=\"Id_Input \"> <br>\r\n            <input type=\"text\" v-model=\"form.name\" placeholder=\"Name \" />\r\n            <button type=\"submit\">Add</button>\r\n        </from>\r\n\r\n        <h2>List Users</h2>\r\n        <!-- Table -->\r\n        <table>\r\n            <thead>\r\n            <tr>\r\n                <th>ID</th>\r\n                <th>Name</th>\r\n            </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"user in users\" :key=\"user.id\">\r\n                    <td>{{ user.id }}</td>\r\n                    <td>{{ user.name }}</td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n    export default {\r\n        name: 'MyMain',\r\n        data() {\r\n            return {\r\n                form:{\r\n                    id:\"\",\r\n                    name:\"\",\r\n                },\r\n                users:[],\r\n            }\r\n        },\r\n        \r\n        methods:{\r\n            getItem(){\r\n                axios.get(\"http://localhost:3000/users\")\r\n                .then((response) => {\r\n                    this.users = response.data;\r\n                    console.log(\"data\", response.data);\r\n                })\r\n                .catch((error) => {\r\n                    console.log(\"Error Get Data \", error);\r\n                })\r\n            },\r\n            postItem(){\r\n                axios\r\n                .post(\"http://localhost:3000/users\", this.form)\r\n                .then(() => {\r\n                    this.getItem();\r\n                    this.form.id= \"\";\r\n                    this.form.name= \"\";\r\n                    alert(\"Data berhasil dipost\");\r\n                })\r\n                .catch((error) => {\r\n                    console.log(\"Error Get Data \", error);\r\n                })\r\n            },\r\n        },\r\n        \r\n        mounted() {\r\n            this.getItem();\r\n        }\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n    .tabelnya {\r\n        width: 100%;\r\n        align-items: ClientRequest;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module"}